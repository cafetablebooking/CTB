{
  "version": 2,
  "projects": {
    "owner-admin": {
      "root": "apps/owner-admin",
      "sourceRoot": "apps/owner-admin/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/owner-admin",
            "index": "apps/owner-admin/src/index.html",
            "main": "apps/owner-admin/src/main.tsx",
            "polyfills": "apps/owner-admin/src/polyfills.ts",
            "tsConfig": "apps/owner-admin/tsconfig.app.json",
            "assets": [
              "apps/owner-admin/src/favicon.ico",
              "apps/owner-admin/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/owner-admin/src/environments/environment.ts",
                  "with": "apps/owner-admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "owner-admin:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "owner-admin:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/owner-admin/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/owner-admin"],
          "options": {
            "jestConfig": "apps/owner-admin/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "owner-admin-e2e": {
      "root": "apps/owner-admin-e2e",
      "sourceRoot": "apps/owner-admin-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/owner-admin-e2e/cypress.json",
            "tsConfig": "apps/owner-admin-e2e/tsconfig.e2e.json",
            "devServerTarget": "owner-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "owner-admin:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/owner-admin-e2e/**/*.{js,ts}"]
          }
        }
      }
    },

    "client": {
      "root": "apps/client",
      "sourceRoot": "apps/client",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/client",
            "outputPath": "dist/apps/client"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "client:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "client:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "client:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/client/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/client"],
          "options": {
            "jestConfig": "apps/client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "client-e2e": {
      "root": "apps/client-e2e",
      "sourceRoot": "apps/client-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/client-e2e/cypress.json",
            "tsConfig": "apps/client-e2e/tsconfig.e2e.json",
            "devServerTarget": "client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "client:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/client-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "firebase-auth": {
      "root": "libs/firebase-auth",
      "sourceRoot": "libs/firebase-auth/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/firebase-auth/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/firebase-auth"],
          "options": {
            "jestConfig": "libs/firebase-auth/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "auth-context": {
      "root": "libs/auth-context",
      "sourceRoot": "libs/auth-context/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/auth-context/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/auth-context"],
          "options": {
            "jestConfig": "libs/auth-context/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },

    "auth-crud": {
      "root": "libs/auth-crud",
      "sourceRoot": "libs/auth-crud/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/auth-crud/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/auth-crud"],
          "options": {
            "jestConfig": "libs/auth-crud/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "owner-admin"
}
